datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator db {
    provider = "go run github.com/prisma/prisma-client-go"
}

model Disease {
    name        String  @id
    part        String
    symptoms    Symptoms[]
}

model Symptoms {
    name        String  @id 
    disease     Disease[]
}